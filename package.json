{
  "name": "restaf",
  "version": "1.7.5",
  "description": "Library for writing SAS Viya application using REST APIs",
  "author": "Deva Kumaraswamy <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "keywords": [
    "restaf",
    "SAS",
    "Viya",
    "REST API"
  ],
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6.10.0",
    "yarn": ">=1.17.3"
  },
  "main": "lib/restaf",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/serverless-*/**",
      "**/registerclient/**",
      "**/viyacaddy/**"
    ]
  },
  "repository": "https://github.com/sassoftware/restaf.git",
  "bugs": {
    "url": "https://github.com/sassoftware/restaf/issues"
  },
  "scripts": {
    "test": "cd ./packages/examples && node",
    "debug": "cd ./packages/examples && node --inspect-brk",
    "restaf:build": "cd ./packages/restaf && yarn run build && npx cp -r ./dist/restaf.js ../uidemos/public/shared/dist/restaf.js",
    "restaflib:build": "cd ./packages/restaflib && yarn run build  && npx cp -r ./dist/restaflib.js ../uidemos/public/shared/dist/restaflib.js",
    "restafuilib:build": "cd ./packages/restafuilib && yarn run build  && npx cp -r ./dist/restafuilib.js ../uidemos/public/shared/dist/restafuilib.js",
    "uidemos:implicit": "cd ./packages/uidemos && npm run implicit",
    "uidemos:code": "cd ./packages/uidemos && npm run code",
    "slsscore": "cd ./packages/serverless-score && getawskey &&sls offline",
    "registerclient": "cd ./packages/registerclient && npm start",
    "viyacaddy": "cd ./packages/viyacaddy && npm start",
    "bldall": "npm run restaf:build && npm run restaflib:build && npm run restafuilib:build"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-do-expressions": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-pipeline-operator": "^7.7.7",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-regenerator": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/plugin-transform-spread": "^7.7.4",
    "@babel/plugin-transform-strict-mode": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-redux-saga": "^1.1.3",
    "jsdoc": "^3.6.3",
    "lerna": "^3.20.2",
    "prettyjson": "^1.2.1",
    "rimraf": "^3.0.0",
    "set-versions": "^1.0.3",
    "shelljs": "^0.8.3",
    "showdown": "^1.9.1",
    "shx": "^0.3.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^15.1.0"
  },
  "resolutions": {
    "**/lodash": ">=4.17.15",
    "**/mem": "^5.1.1",
    "**/core-js": "^3.6.1" 
  },
  "dependencies": {
    "@sassoftware/restaf-server": "^7.2.0",
    "aws-sdk": "^2.597.0",
    "axios": "^0.19.0",
    "jwt-decode": "^2.2.0",
    "react-json-pretty": "^2.2.0",
    "serverless": "^1.60.4"
  }
}
